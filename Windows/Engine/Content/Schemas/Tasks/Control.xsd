          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="Control.GetContentFromEndpoint">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="BaseTaskNodeType">
