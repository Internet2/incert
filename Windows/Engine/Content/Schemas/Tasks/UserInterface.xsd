  <xs:simpleType name="LinkPolicies">
    <xs:annotation>
      <xs:documentation>Determines how the embedded brower will handle hyperlinks that do not have an explicit 'target' attribute set</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="None">
        <xs:annotation>
          <xs:documentation>All hyperlinks will be opened in an external browser window.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="All">
        <xs:annotation>
          <xs:documentation>All hyperlinks will be opened in the embedded browser</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Internal">
        <xs:annotation>
          <xs:documentation>'resource://' and 'archive://' hyperlinks will be opened in the embedded browser. All other hyperlinks will be opened in an external browser window</xs:documentation>
          <xs:documentation>This is the default value</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="InternalAndSameHost">
        <xs:annotation>
          <xs:documentation>'resource://' and 'archive://' hyperlinks will be opened in the embedded browser, and hyperlinks with the same host as the original url will be opened in embedded browser. All other hyperlinks will be opened in an external browser window</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
